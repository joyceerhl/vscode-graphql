{
  "name": "vscode-graphql-notebook",
  "publisher": "joyceerhl",
  "displayName": "GraphQL Notebook",
  "license": "MIT",
  "description": "Run and save GraphQL queries and mutations",
  "galleryBanner": {
    "color": "#032539",
    "theme": "dark"
  },
  "keywords": [
    "graphql",
    "Notebooks"
  ],
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.65.0"
  },
  "repository": {
    "url": "https://github.com/joyceerhl/vscode-graphql-notebook"
  },
  "preview": true,
  "categories": [
    "Notebooks"
  ],
  "activationEvents": [
    "onNotebook:gqlnb",
    "onCommand:gqlnb.createNew"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "gqlnb.createNew",
        "title": "Create New GraphQL Notebook",
        "shortTitle": "GraphQL Notebook",
        "category": "GraphQL"
      }
    ],
    "menus": {
      "file/newFile": [
        {
          "command": "gqlnb.createNew",
          "group": "notebook"
        }
      ]
    },
    "notebooks": [
      {
        "type": "gqlnb",
        "displayName": "GraphQL Notebook",
        "priority": "default",
        "selector": [
          {
            "filenamePattern": "*.gqlnb"
          }
        ]
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile -- --minify",
    "compile": "esbuild ./src/extension.ts --bundle --outfile=out/extension.js --external:vscode --format=cjs --platform=node",
    "build": "npm run compile -- --sourcemap",
    "watch": "npm run build --watch",
    "test": "npm run compile && node ./node_modules/vscode/bin/test",
    "vsce:package": "vsce package",
    "env:source": "export $(cat .envrc | xargs)",
    "vsce:publish": "vsce publish",
    "open-vsx:publish": "ovsx publish -p \"$OPEN_VSX_ACCESS_TOKEN\"",
    "publish": "npm run vsce:publish && npm run open-vsx:publish",
    "upgrade-interactive": "npx npm-check -u"
  },
  "devDependencies": {
    "@types/capitalize": "2.0.0",
    "@types/dotenv": "8.2.0",
    "@types/mocha": "5.2.7",
    "@types/node": "16.11.26",
    "@types/node-fetch": "3.0.3",
    "@types/vscode": "1.62.0",
    "@types/ws": "8.2.2",
    "esbuild": "0.13.15",
    "ovsx": "0.3.0",
    "tslint": "5.20.1",
    "typescript": "4.4.4",
    "vsce": "2.6.7",
    "vscode": "1.1.37"
  },
  "dependencies": {
    "@changesets/changelog-github": "0.4.3",
    "@changesets/cli": "2.21.1",
    "@graphql-tools/load": "7.5.2",
    "@graphql-tools/url-loader": "7.8.0",
    "@graphql-tools/wrap": "8.4.3",
    "@urql/core": "2.3.6",
    "babel-polyfill": "6.26.0",
    "capitalize": "2.0.4",
    "dotenv": "10.0.0",
    "escape-html": "1.0.3",
    "graphql": "15.8.0",
    "graphql-config": "4.1.0",
    "graphql-language-service-server": "2.7.15",
    "graphql-tag": "2.12.6",
    "graphql-ws": "5.5.5",
    "node-fetch": "2.6.7",
    "vscode-languageclient": "5.2.1",
    "ws": "8.2.3"
  },
  "resolutions": {
    "graphql-config": "4.1.0"
  }
}
